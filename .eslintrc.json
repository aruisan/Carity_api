{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "google"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      // "no-undef": ["error", "always"],
      "semi": ["error", "always"],
      "quotes": ["error", "double"]
  }
}
